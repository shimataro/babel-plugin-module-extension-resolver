{
  "name": "babel-plugin-module-extension-resolver",
  "version": "0.2.0",
  "description": "Babel plugin that resolves and maps module extensions.",
  "keywords": [
    "babel",
    "babel-plugin"
  ],
  "homepage": "https://github.com/shimataro/babel-plugin-module-extension-resolver",
  "bugs": {
    "url": "https://github.com/shimataro/babel-plugin-module-extension-resolver/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shimataro/babel-plugin-module-extension-resolver.git"
  },
  "license": "MIT",
  "main": "./dist/index",
  "scripts": {
    "build": "run-p build:*",
    "build:cjs": "npm run build-core",
    "build:esm": "cross-env BABEL_ENV=esm npm run build-core -- --keep-file-extension",
    "build-core": "babel src --out-dir ./dist --source-maps --delete-dir-on-start --verbose",
    "check-updates": "ncu -x @babel/preset-env",
    "lint": "run-p lint:*",
    "lint:es": "eslint ./src --ext .mjs",
    "lint:md": "markdownlint . --ignore node_modules --ignore examples",
    "lint:yaml": "yamllint .*.yml *.yml --ignore=node_modules/**/*.yml --ignore=node_modules/**/*.yaml",
    "verify": "run-p lint"
  },
  "dependencies": {
    "core-js": "3.6.1"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@babel/core": "7.7.7",
    "@babel/preset-env": "7.7.5",
    "babel-eslint": "10.0.3",
    "cross-env": "6.0.3",
    "eslint": "6.8.0",
    "eslint-plugin-import": "2.19.1",
    "jest": "24.9.0",
    "markdownlint-cli": "0.20.0",
    "npm-check-updates": "4.0.1",
    "npm-run-all": "4.1.5",
    "yaml-lint": "1.2.4"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
