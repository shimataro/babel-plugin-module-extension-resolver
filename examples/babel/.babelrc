{
  "presets": [
    ["@babel/preset-env", {"targets": {"node": "4.0.0"}, "useBuiltIns": "usage", "corejs": 3}]
  ],
  "env": {
    "esm": {
      "presets": [
        ["@babel/preset-env", {"targets": {"node": "8.5.0"}, "useBuiltIns": "usage", "corejs": 3, "modules": false}]
      ],
      "plugins": [
        ["module-extension-resolver", {
          "extensionsToKeep": [".mjs", ".json"]
        }]
      ]
    }
  }
}
